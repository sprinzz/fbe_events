<% content_for(:html_title) { 'New event' } %>

  <div class="section bg-white ct-menu">
  <div class="section__inner">

  <div class="content-block content-block--sml">

      <p><%= link_to("<< Back to events list", events_path, :class => 'back-link') %></p>
      </div>




<div class="content-block">
<div class="form">


  <%= form_for(@event) do |f| %>

    <%= render(:partial => 'form', :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= f.submit("Create event") %>
    </div>

  <% end %>
  </div>
</div>
</div>
</div>
