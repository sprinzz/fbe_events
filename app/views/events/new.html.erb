<main>

  <div class="content-block content-block--sml">
    <h1>
        New Event
      </h1>
      <p><%= link_to("<< Back to List", events_path, :class => 'back-link') %></p>
      </div>




<div class="content-block">
<div class="form">


  <%= form_for(@event) do |f| %>

    <%= render(:partial => 'form', :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= f.submit("Create event") %>
    </div>

  <% end %>
  </div>
</div>
</main>
